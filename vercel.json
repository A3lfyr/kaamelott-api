{
    "version": 2,
    "builds": [
      {
        "src": "index.ts",
        "use": "@now/node"
      }
    ],
    "routes": [
      {
        "src": "/quote/(.*)",
        "dest": "index.ts"
      },
      {
        "src": "/quotes/(.*)",
        "dest": "index.ts"
      },
      {
        "src": "/characters/(.*)",
        "dest": "index.ts"
      },
      {
        "src": "/casting/(.*)",
        "dest": "index.ts"
      }
    ]
  }