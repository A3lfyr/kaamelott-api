{
    "version": 2,
    "framework": null,
    "outputDirectory": "public",
    "builds": [
        {
          "src": "index.ts",
          "use": "@now/node"
        }
    ],
    "routes": [
        {
          "src": "/test",
          "dest": "public/index.html"
        },
        {
          "src": "/test2",
          "dest": "index.html"
        },
        {
          "src": "/(.*)",
          "dest": "index.ts"
        }
      ]
  }