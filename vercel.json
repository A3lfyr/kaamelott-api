{
    "version": 2,
    "framework": null,
    "installCommand": "npm install",
    "outputDirectory": "public",
    "builds": [
        {
          "src": "index.js",
          "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "public/index.html"
        },
        {
            "src": "/quote/(.*)",
            "dest": "index.ts"
        },
        {
            "src": "/quotes/(.*)",
            "dest": "index.ts"
        },
        {
            "src": "/characters/(.*)",
            "dest": "index.ts"
        },
        {
            "src": "/casting/(.*)",
            "dest": "index.ts"
        }
    ]
  }