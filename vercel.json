{
    "version": 2,
    "builds": [
      {
        "src": ".",
        "use": "@now/node"
      }
    ],
    "framework": null,
    "installCommand": "npm install",
    "outputDirectory": "public",
    "routes": [
        {
            "src": "/",
            "dest": "public/index.html"
        },
        {
            "src": "/quote/(.*)",
            "dest": "index.ts"
        },
        {
            "src": "/quotes/(.*)",
            "dest": "index.ts"
        },
        {
            "src": "/characters/(.*)",
            "dest": "index.ts"
        },
        {
            "src": "/casting/(.*)",
            "dest": "index.ts"
        }
    ]
  }